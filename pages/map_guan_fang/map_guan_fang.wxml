<!--pages/map_guan_fang/map_guan_fang.wxml-->
<map
  id="map"
  longitude="{{long}}"
  latitude="{{lati}}"
  scale="18"
  controls="{{controls}}"
  bindcontroltap="controltap"
  markers="{{markers}}"
  bindmarkertap="markertap"
  polyline="{{polyline}}"
  bindregionchange="regionchange"
  show-location
  style="width: 100%; height: 300px;"
></map>